@model IEnumerable<CakeShop.Models.Cake>

<h1>Piece of Cakes</h1>
@if (Model.Count() > 0)
{
    <div class="row">
        @foreach (var cake in Model)
        {
            @await Html.PartialAsync("CakeView", cake);
        }
    </div>
}
else
{
<p>We are sorry, we couldn't find any cake! &#128546; </p>
}
